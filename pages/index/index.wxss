.container{
  width: 100%;
  height: 100vh !important;
}
.BgImg{
  width: 100% !important;
  height: 100% !important;
}
.SlideBlock{
  width: 750rpx;
  height: calc(100% - 15%);
  position: fixed;
  /* bottom: 120px; */
  top: 0;
  left: -30px;
  /* background: blue; */
}

@media screen and (max-width: 834px) {
  .swiper-lb{
    height: 100%;
    width: 75%;
    overflow: hidden;
    position: relative;
  }
  .BotBlock{
    width: 100%;
    height: 12%;
    position: fixed;
    left: 0;
    bottom:0;
  }

