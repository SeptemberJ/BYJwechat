.container{
  width: 100%;
  height: 100vh !important;
  display: flex;
  justify-content: center;
}
.TopBlock{
  width: 100%;
  height: calc(100vh - 80px);
  /* position: absolute;
  top: 0;
  left: 0; */
  display: flex;
  justify-content: center;
}
.BotBlock{
  width: 100%;
  height: 80px;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  position: absolute;
  bottom: 0;
  left: 0;
}
.Bt_top , .Bt_bottom{
  width: 100%;
  height: 40px;
  display: flex;
  align-items: center;
  justify-content: center;
}
.BotBlock_bt{
  width: 50%;
  height: 100%;
}
